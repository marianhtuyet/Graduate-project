<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="standard_material_tree" model="ir.ui.view">
        <field name="name">standard.material.tree</field>
        <field name="model">standard.material</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="name2"/>
                <field name="protein_a"/>
                <field name="protein_v"/>
                <field name="lipit_a"/>
                <field name="lipit_v"/>
                <field name="gluco"/>
                <field name="calo"/>
                <field name="rate_min"/>
                <field name="rate_max"/>
            </tree>
        </field>
    </record>

    <record id="standard_material_view" model="ir.ui.view">
        <field name="name">standard.material.view</field>
        <field name="model">standard.material</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name2"/>
                            <field name="name"/>
                            <field name="protein_a"/>
                            <field name="protein_v"/>
                            <field name="lipit_a"/>
                            <field name="lipit_v"/>
                        </group>
                        <group>
                            <field name="gluco"/>
                            <field name="calo"/>
                            <field name="rate_min"/>
                            <field name="rate_max"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
