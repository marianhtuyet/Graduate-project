<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="type_food_tree" model="ir.ui.view">
        <field name="name">type.food.tree</field>
        <field name="model">type.food</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="name2"/>
                <field name="group_name"/>
            </tree>
        </field>
    </record>

    <record id="type_food_view" model="ir.ui.view">
        <field name="name">type.food.view</field>
        <field name="model">type.food</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="name2"/>
                            <field name="group_name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
