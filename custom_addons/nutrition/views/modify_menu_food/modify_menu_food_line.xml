<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="modify_menu_food_line_tree" model="ir.ui.view">
        <field name="name">modify.menu.food.line.tree</field>
        <field name="model">modify.menu.food.line</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="nutrition_id"/>
                <field name="quantity"/>
                <field name="price"/>
                <field name="qty_g"/>
                <field name="amount"/>
                <field name="qty_eat"/>
                <field name="constant"/>
                <field name="qty_buy"/>
                <field name="protein_a"/>
                <field name="protein_v"/>
                <field name="lipit_a"/>
                <field name="lipit_v"/>
                <field name="gluco"/>
                <field name="calo"/>
                <field name="qty_uom"/>
            </tree>
        </field>
    </record>

    <record id="menu_modify_food_line_view" model="ir.ui.view">
        <field name="name">modify.menu.food.line.view</field>
        <field name="model">modify.menu.food.line</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="modify_menu_food_id"/>
                            <field name="nutrition_id"/>
                            <field name="quantity"/>
                            <field name="qty_g"/>
                            <field name="price"/>
                            <field name="amount"/>
                            <field name="qty_eat"/>
                            <field name="constant"/>
                            <field name="qty_buy"/>

                        </group>
                        <group>
                            <field name="protein_a"/>
                            <field name="protein_v"/>
                            <field name="lipit_a"/>
                            <field name="lipit_v"/>
                            <field name="gluco"/>
                            <field name="calo"/>
                            <field name="qty_uom"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
